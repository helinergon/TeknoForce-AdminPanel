@page "/pages/gallery"
@model GalleryModel


@{
    ViewBag.title = "Rizz";
}


@section styles {
    <link href="/libs/tobii/css/tobii.min.css" rel="stylesheet" type="text/css"/>
}


<div class="row justify-content-center">
    <div class="col-12">
        <div class="card">
            <div class="card-header">
                <div class="row align-items-center">
                    <div class="col">
                        <h4 class="card-title">Gallery</h4>
                    </div><!--end col-->
                </div> <!--end row-->
            </div><!--end card-header-->
            <div class="card-body pt-0">
                <div class="row g-0" id="grid">
                    <div class="col-md-4 col-lg-3 picture-item">
                        <a class="lightbox" href="/images/extra/card/img-1.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-1.jpg"/>
                        </a>
                    </div>
                    <div class="col-md-4 col-lg-3 picture-item picture-item--overlay">
                        <a class="lightbox" href="/images/extra/card/img-2.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-2.jpg"/>
                        </a>
                    </div>
                    <div class="col-md-4 col-lg-3 picture-item">
                        <a class="lightbox" href="/images/extra/card/img-3.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-3.jpg"/>
                        </a>
                    </div>
                    <div class="col-md-4 col-lg-3 picture-item picture-item--h2">
                        <a class="lightbox" href="/images/extra/card/img-4.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-4.jpg"/>
                        </a>
                    </div>
                    <div class="col-md-4 col-lg-3 picture-item">
                        <a class="lightbox" href="/images/extra/card/img-5.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-5.jpg"/>
                        </a>
                    </div>
                    <div class="col-md-4 col-lg-3 picture-item picture-item--overlay">
                        <a class="lightbox" href="/images/extra/card/img-6.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-6.jpg"/>
                        </a>
                    </div>
                    <div class="col-md-4 col-lg-3 picture-item picture-item--h2">
                        <a class="lightbox" href="/images/extra/card/img-2.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-2.jpg"/>
                        </a>
                    </div>
                    <div class="col-md-4 col-lg-3 picture-item picture-item--h2">
                        <a class="lightbox" href="/images/extra/card/img-1.jpg">
                            <img alt="" class="img-fluid" src="/images/extra/card/img-1.jpg"/>
                        </a>
                    </div>
                </div>
            </div><!--end card-body-->
        </div><!--end card-->
    </div> <!--end col-->
</div><!--end row-->


@section scripts {
    <script src="/libs/tobii/js/tobii.min.js"></script>
    <script>
        const tobii = new Tobii()
    </script>
}
